use_frameworks!

def common_pods
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'RealmSwift'
end

target 'Dictamed' do
    platform :ios, '9.0'
    common_pods
    pod 'SVProgressHUD'
    pod 'DGElasticPullToRefresh'
    pod 'MCSwipeTableViewCell'
    pod 'IBAnimatable'
end

target 'Watch Extension' do
    platform :watchos, '2.0'
    common_pods
    pod 'EMTLoadingIndicator'
end
